##############################################################################
# Oracle Enterprise Manager Cloud Control 13.5 Silent Installation
# Response File Template
#
# Usage:
#   ./em13500_linux64.bin -silent -responseFile /path/to/oms13.5_install.rsp
#
# IMPORTANT: Replace all placeholder values with your actual configuration
#            before running the installation
##############################################################################

# Response File Version
RESPONSEFILE_VERSION=2.2.1.0.0

##############################################################################
# Installation Type and Location
##############################################################################

# Installation Type
# Options: NOSEED (OMS with new repository), SEED (OMS with seed database)
EM_INSTALL_TYPE=NOSEED

# Oracle Inventory Group
UNIX_GROUP_NAME=oinstall

# Oracle Inventory Location
INVENTORY_LOCATION=/u01/app/oracle/oraInventory

# Oracle Middleware Home Location
ORACLE_MIDDLEWARE_HOME_LOCATION=/u01/app/oracle/middleware13.5

# Oracle Instance Home Location (OMS Instance Base)
ORACLE_INSTANCE_HOME_LOCATION=/u01/app/oracle/gc_inst

# Agent Base Directory
AGENT_BASE_DIR=/u01/app/oracle/agent

# Oracle Hostname (FQDN recommended)
ORACLE_HOSTNAME=ip-10-72-56-75.vpc.internal

##############################################################################
# Security Updates Configuration
##############################################################################

# Security Updates via My Oracle Support
# Set to true if you want to receive security updates via My Oracle Support
SECURITY_UPDATES_VIA_MYORACLESUPPORT=false

# Decline Security Updates
# Set to true to decline security updates
DECLINE_SECURITY_UPDATES=true

# Install Updates Selection
# Options: skip, apply, apply_selected
INSTALL_UPDATES_SELECTION=skip

##############################################################################
# WebLogic Server Configuration
##############################################################################

# WebLogic Admin Server Username
WLS_ADMIN_SERVER_USERNAME=weblogic

# WebLogic Admin Server Password
WLS_ADMIN_SERVER_PASSWORD=Intuit_123

# WebLogic Admin Server Password Confirmation
WLS_ADMIN_SERVER_CONFIRM_PASSWORD=Intuit_123

# Node Manager Password
NODE_MANAGER_PASSWORD=Intuit_123

# Node Manager Password Confirmation
NODE_MANAGER_CONFIRM_PASSWORD=Intuit_123

##############################################################################
# Database Configuration
##############################################################################

# Database Hostname
DATABASE_HOSTNAME=10.72.62.46

# Database Listener Port
LISTENER_PORT=1441

# Database Service Name or SID
# Use SERVICE_NAME for container databases, SID for non-container databases
SERVICENAME_OR_SID=emmsys01

# SYS Password (for repository creation)
SYS_PASSWORD=Intuit_123

# SYSMAN Password (Management Repository user)
SYSMAN_PASSWORD=Intuit_123

# SYSMAN Password Confirmation
SYSMAN_CONFIRM_PASSWORD=Intuit_123

##############################################################################
# Repository Configuration
##############################################################################

# Deployment Size
# Options: SMALL, MEDIUM, LARGE
# SMALL: < 1000 targets, MEDIUM: 1000-10000 targets, LARGE: > 10000 targets
DEPLOYMENT_SIZE=MEDIUM

# Management Tablespace Location
# Full path to the datafile for management tablespace
##MANAGEMENT_TABLESPACE_LOCATION=/data/21c/oradata/EMMSYS/pdbseed/management01.dbf
MANAGEMENT_TABLESPACE_LOCATION="/data/files/mgmt.dbf"

# Configuration Data Tablespace Location
# Full path to the datafile for configuration data tablespace
##CONFIGURATION_DATA_TABLESPACE_LOCATION=/data/21c/oradata/EMMSYS/pdbseed/data01.dbf
CONFIGURATION_DATA_TABLESPACE_LOCATION="/data/files/mgmt_ecm_depot1.dbf"

# JVM Diagnostics Tablespace Location
# Full path to the datafile for JVM diagnostics tablespace
##JVM_DIAGNOSTICS_TABLESPACE_LOCATION=/data/21c/oradata/EMMSYS/pdbseed/jvm01.dbf
JVM_DIAGNOSTICS_TABLESPACE_LOCATION="/data/files/mgmt_deepdive.dbf"

##############################################################################
# Agent Configuration
##############################################################################

# Agent Registration Password
# Password for agents to register with OMS
AGENT_REGISTRATION_PASSWORD=Intuit_123

# Agent Registration Password Confirmation
AGENT_REGISTRATION_CONFIRM_PASSWORD=Intuit_123

##############################################################################
# Software Library Configuration
##############################################################################

# Configure Oracle Software Library
# Set to true to configure Software Library
CONFIGURE_ORACLE_SOFTWARE_LIBRARY=true

# Software Library Location
# Location for Oracle Software Library (if configured)
SOFTWARE_LIBRARY_LOCATION=/u01/app/oracle/swlib

##############################################################################
# Configuration Type
##############################################################################

# Configuration Type
# Options: TYPICAL, ADVANCED
# TYPICAL: Uses default settings
# ADVANCED: Allows custom configuration
CONFIGURATION_TYPE=ADVANCED

##############################################################################
# Shared Location Configuration
##############################################################################

# Configure Shared Location for BI Publisher
# Set to true if you want to configure shared location for BI Publisher
CONFIGURE_SHARED_LOCATION_BIP=false

##############################################################################
# Plugin Selection
##############################################################################

# Plugin Selection
# Format: {plugin1,plugin2,plugin3}
# Example: {oracle.sysman.db,oracle.sysman.ias}
# Leave empty {} for default plugins
PLUGIN_SELECTION={}

##############################################################################
# Upgrade Configuration
##############################################################################

# Upgrade Flag
# Set to true if this is an upgrade installation
b_upgrade=false

##############################################################################
# Additional Notes:
#
# 1. Replace all "Intuit_123" passwords with secure passwords
# 2. Update all paths to match your environment
# 3. Ensure database is running and accessible before installation
# 4. Verify all directories exist and have proper permissions
# 5. For container databases (CDB), use SERVICE_NAME format
# 6. For non-container databases, use SID format
# 7. Ensure sufficient disk space for tablespaces
# 8. Review deployment size based on expected number of targets
#
# Example for Container Database (CDB):
#   SERVICENAME_OR_SID=orclpdb (PDB service name)
#
# Example for Non-Container Database:
#   SERVICENAME_OR_SID=orcl (SID)
#
